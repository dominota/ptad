 
cd OpenTLD
mkdir build
cd build
cmake .. 
make


%To run from camera
./ptad -p ../parameters.yml -vc 0 


%To run from video   
./ptad -p  ../parameters.yml -vs ../datasets/06_car.avi  

%To run from video   init bounding box from file
./ptad -p  ../parameters.yml -vs ../datasets/06_car.avi -b ../datasets/06_car/init.txt

 %To run from image sequence    
./ptad -p ../parameters.yml -i ../datasets/06_car/ 

 %To run from image sequence    init bounding box from file 
./ptad -p ../parameters.yml -i ../datasets/06_car/ -b ../datasets/06_car/init.txt
 
 

